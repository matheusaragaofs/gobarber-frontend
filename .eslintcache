ndleSubmit = useCallback(\n    async (data: ResetPasswordFormData) => {\n      try {\n        formRef.current?.setErrors({});\n\n        const schema = Yup.object().shape({\n          password: Yup.string().required('Senha obrigatória'),\n          password_confirmation: Yup.string().oneOf(\n            [Yup.ref('password'), null],\n            'Confirmação incorreta',\n          ),\n        });\n\n        await schema.validate(data, {\n          abortEarly: false,\n        });\n\n        const { password, password_confirmation } = data;\n        const token = location.search.replace('?token=', '');\n\n        if (!token) {\n          console.log(token);\n          throw new Error();\n        }\n\n        await api.post('/password/reset', {\n          password,\n          password_confirmation,\n          token,\n        });\n\n        history.push('/');\n      } catch (err) {\n        if (err instanceof Yup.ValidationError) {\n          const errors = getValidationErrors(err);\n          formRef.current?.setErrors(errors);\n\n          return;\n        }\n\n        addToast({\n          type: 'error',\n          title: 'Erro ao resetar senha',\n          description: 'Ocorreu um erro ao resetar sua senha, tente novamente.',\n        });\n      }\n    },\n    [addToast, history, location.search],\n  );\n\n  return (\n    <Container>\n      <Content>\n        <AnimationContainer>\n          <img src={logoImg} alt=\"GoBarber\" />\n\n          <Form ref={formRef} onSubmit={handleSubmit}>\n            <h1>Resetar senha</h1>\n\n            <Input\n              name=\"password\"\n              icon={FiLock}\n              type=\"password\"\n              placeholder=\"Nova senha\"\n            />\n\n            <Input\n              name=\"password_confirmation\"\n              icon={FiLock}\n              type=\"password\"\n              placeholder=\"Confirmação da senha\"\n            />\n\n            <Button type=\"submit\">Alterar senha</Button>\n          </Form>\n        </AnimationContainer>\n      </Content>\n\n      <Background />\n    </Container>\n  );\n};\n\nexport default ResetPassword;\n","C:\\Users\\mathe\\Desktop\\Matheus\\Rocketseat\\gobarber-frontend\\src\\pages\\ResetPassword\\styles.ts",[],"C:\\Users\\mathe\\Desktop\\Matheus\\Rocketseat\\gobarber-frontend\\src\\pages\\Dashboard\\styles.ts",[],"C:\\Users\\mathe\\Desktop\\Matheus\\Rocketseat\\gobarber-frontend\\src\\pages\\Profile\\index.tsx",[],"C:\\Users\\mathe\\Desktop\\Matheus\\Rocketseat\\gobarber-frontend\\src\\pages\\Profile\\styles.ts",[],{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","replacedBy":"147"},{"ruleId":"148","replacedBy":"149"},{"ruleId":"150","replacedBy":"151"},{"ruleId":"152","replacedBy":"153"},{"ruleId":"144","replacedBy":"154"},{"ruleId":"146","replacedBy":"155"},{"ruleId":"148","replacedBy":"156"},{"ruleId":"150","replacedBy":"157"},{"ruleId":"152","replacedBy":"158"},{"ruleId":"159","severity":1,"message":"160","line":53,"column":11,"nodeType":"161","messageId":"162","endLine":53,"endColumn":22},"lines-around-directive",["163"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],["163"],[],[],[],[],"no-console","Unexpected console statement.","MemberExpression","unexpected","padding-line-between-statements"]